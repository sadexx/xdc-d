    "argon2": "^0.40.3",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.56.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "express-session": "^1.18.2",
    "google-auth-library": "^9.15.1",
    "ioredis": "^5.7.0",
    "jwks-rsa": "^3.2.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "nodemailer-express-handlebars": "^7.0.0",
    "passport-custom": "^1.1.1",
